{
	"rust-analyzer": {
		"diagnostics": {
			"disabled": ["macro-error", "unresolved-macro-call", "unresolved-import", "incorrect-ident-case", "unresolved-proc-macro", "missing-ok-or-some-in-tail-expr", "missing-unsafe", "mismatched-arg-count", "type-mismatch"]
		},
		"cargo": {
			"runBuildScripts": true
		},
		"procMacro": {
			"enable": true
		},
		"checkOnSave": {
			"command": "clippy",
			"allTargets": false
		},
		"server": {
			"extraEnv": {
				"CARGO_TARGET_DIR": "target/rust-analyzer-check"
			}
		}
	}
}
